#. type: Content of: <article><section><variablelist><varlistentry><term>
#: ./English/Filters.xml:1231
msgid "File Filters"
msgstr ""

#. type: Content of: <article><section><variablelist><varlistentry><listitem><para>
#: ./English/Filters.xml:1234
msgid "Choose this tab to work with file filters for folder compare operations."
msgstr ""

#. type: Content of: <article><section><variablelist><varlistentry><term>
#: ./English/Filters.xml:1240
msgid "Line Filters"
msgstr ""

#. type: Content of: <article><section><variablelist><varlistentry><listitem><para>
#: ./English/Filters.xml:1243
msgid "Choose this tab to work with line filters for file compare operations."
msgstr ""

#. type: Content of: <article><section><variablelist><varlistentry><term>
#: ./English/Filters.xml:1249
msgid "Substitution Filters"
msgstr ""

#. type: Content of: <article><section><variablelist><varlistentry><listitem><para>
#: ./English/Filters.xml:1252
msgid "Choose this tab to work with substitution filters for file compare operations."
msgstr ""

