msgid ""
"Adds two functions to the <menuchoice> <guimenu>Plugins</guimenu> "
"<guisubmenu>Scripts</guisubmenu> </menuchoice> menu:"
msgstr ""
"Ajoute deux fonctions au menu <menuchoice> <guimenu>Plug-ins</guimenu> "
"<guisubmenu>Scripts</guisubmenu> </menuchoice> :"

#. type: Content of: <article><section><section><itemizedlist><listitem><simpara>
#: English/Plugins.xml:2502
msgid ""
"<guimenuitem>Insert Date</guimenuitem> insert date in the current locale "
"format."
msgstr ""
"<guimenuitem>Insérer la date</guimenuitem> insère la date au format régional "
"actuel."

#. type: Content of: <article><section><section><itemizedlist><listitem><simpara>
#: English/Plugins.xml:2507
msgid ""
"<guimenuitem>Insert Time</guimenuitem> insert time in the current locale "
"format."
msgstr ""
"<guimenuitem>Insérer l'heure</guimenuitem> insère l'heure au format régional "
"actuel."

